{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "webpack-dev-server --watch-poll",
    "prod": "yarn run clean && cross-env NODE_ENV=production webpack -p",
    "clean": "rimraf ./dist/*",
    "test": "./node_modules/.bin/mocha --require ./test/setup.js ./src/**/*.spec.js",
    "test:watch": "./node_modules/.bin/mocha --require ./test/setup.js ./src/**/*.spec.js --watch ./src/**/*.js",
    "coverage": "./node_modules/.bin/nyc --reporter=lcov --reporter=text ./node_modules/.bin/mocha --require ./test/setup.js ./src/**/*.spec.js"
  },
  "author": "Razum",
  "devDependencies": {
    "autoprefixer-loader": "^3.2.0",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-istanbul": "^4.1.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "chai": "^3.5.0",
    "cross-env": "^4.0.0",
    "css-loader": "^0.28.0",
    "enzyme": "^2.8.2",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "extract-text-webpack-plugin": "^2.1.0",
    "html-webpack-plugin": "^2.28.0",
    "istanbul": "^0.4.5",
    "jsdom": "^9.12.0",
    "mocha": "^3.2.0",
    "node-sass": "^4.5.0",
    "nyc": "^10.2.0",
    "react-addons-test-utils": "^15.5.1",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.3",
    "sinon": "^2.1.0",
    "style-loader": "^0.16.1",
    "webpack": "^2.4.1",
    "webpack-dev-server": "^2.4.3"
  },
  "dependencies": {
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-hot-loader": "3.0.0-beta.6"
  }
